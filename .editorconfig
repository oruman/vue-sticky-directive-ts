root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[**.{json,js,ts,yml,yaml}]
indent_style = space
indent_size = 2

[*.svg]
indent_style = space
indent_size = 2

[**.{js,ts,vue}]
quote_type = double

[*.md]
indent_style = space
indent_size = 4
